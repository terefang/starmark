<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
{% if meta.generator -%}
<meta name="generator" content="{{ meta.generator }}" />
{% endif %}
{%- if meta.creator -%}
<meta name="creator" content="{{ meta.creator }}" />
{% endif %}
{%- if meta.author -%}
<meta name="author" content="{{ meta.author }}" />
{% endif %}
{%- if meta.date -%}
<meta name="date" content="{{ meta.date }}" />
{% endif %}
{%- if meta.keywords -%}
<meta name="keywords" content="{{ meta.keywords }}" />
{% endif %}
{%- if meta.title -%}
<title>{{ meta.title }}</title>
{%- endif %}

{% for _css in cssfiles %}
{{ _css }}
{% endfor %}

</head>

<body>

{% for _body in bodies %}

<section  class=" {{ _body.class }} " data-file="{{ _body.file }}" >
{{ _body.content }}
</section>
{%- if _body.spacer -%}
{{ _body.spacer }}
{%- endif %}

{% endfor %}

</body>
</html>
